<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
 android:versionCode="1"
 android:versionName="2.5.5"
 package="com.orangelabs.rcs.provisioning">
      
	<uses-sdk android:minSdkVersion="5"/>
	
	<uses-permission android:name="com.orangelabs.rcs.permission.RCS"/>	
	
	<application android:icon="@drawable/app_icon" android:label="@string/app_name">
        
        <activity android:name=".Provisioning">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".ProfileProvisioning"/>
        <activity android:name=".StackProvisioning"/>
        <activity android:name=".ServiceProvisioning"/>
		<activity android:name=".LoggerProvisioning"/>

    </application>


</manifest>
